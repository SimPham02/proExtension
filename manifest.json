{
  "name": "pro Extensions",
  "description": "Extension for everyone",
  "version": "1.4",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "features/edge-home/newtab.html"
  },
  "permissions": ["bookmarks", "storage", "tabs", "activeTab", "scripting", "cookies"],
  "host_permissions": ["<all_urls>"]
}